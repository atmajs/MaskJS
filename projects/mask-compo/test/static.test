UTest({
	'!should attach the function to instance' () {
		var spyInner = sinon.spy();
		var spyOuter = sinon.spy();

		var foo = new (Compo({
			someFn: spyInner
		}));

		Compo.attach(foo, 'someFn', spyOuter);
		foo.someFn('bar', 2);

		const expect = [['bar', 2]];
		eq_(spyInner.callCount, 1);
		deepEq_(spyInner.args, expect);
		deepEq_(spyOuter.args, expect);
	}
})