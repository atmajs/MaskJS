UTest({
	'placeholders' () {
		var a = `
			div > @foo;
		`;
		var b = `
			@foo > span;
		`;
		var nodes = Mask.merge(a, b);
		var str = Mask.stringify(nodes);
		eq_(str, 'div>span;');
	},
	'join nodes outside any placeholders' () {
		var a = `
			div > @foo;
		`;
		var b = `
			span;
		`;
		var nodes = Mask.merge(a, b);
		var str = Mask.stringify(nodes);
		eq_(str, 'div;span;');
	},
	'insert nodes into the root placeholder' () {
		var a = `
			div > @placeholder;
		`;
		var b = `
			span;
		`;
		var nodes = Mask.merge(a, b);
		var str = Mask.stringify(nodes);
		eq_(str, 'div>span;');
	}
})