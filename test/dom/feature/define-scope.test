UTest({
	'should create functions with scope for': {
		'... for variables' () {

			var template = `
				define Foo {
					var WIDTH = 10 / 2;

					function onRenderStart () {
						this.width = WIDTH;
					}
				}
			`;

			var Foo = mask.Compo.initialize('Foo');
			eq_(Foo.width, 5);
		},

		'!... for imports' () {
			var template = `
				import foo as '/test/tmpl/modules/data_foo.js';

				define Foo {
					
					function getFoo () {
						return foo;
					}
				}
			`;

			var Foo = mask.Compo.initialize(template);
			debugger;

			deepEq_(Foo.getFoo(), { name: 'Foo'});
		}
	},
})