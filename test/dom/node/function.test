UTest({
	'function node' () {
		var tmpl = `
			:foo {
				function doChange() {
					this.$.text('B');
				}
				slot change () {
					this.doChange();
				}
				
				button x-tap='change' > 'A'
			}
		`;
		
		mask.registerHandler(':foo', Compo({}));
		var dom = mask.render(tmpl);
		return UTest.domtest(dom, `
			with ('button') {
				text A;
				click;
				text B;
			}
		`);
	},
	'serialization' () {
		var tmpl = `
			:foo {
				function doChange() {
					this.$.text('B');
				}
				button > 'A'
			}
		`;
		var str = mask.stringify(mask.parse(tmpl), 4);
		eq_(str.replace(/\s/g, ''), tmpl.replace(/\s/g, ''));
	}
});