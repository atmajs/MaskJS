UTest({
	
	'minify': function(){
		
		var M = Mask.mask
		
		io
			.File
			.read('test/tmpl/stringify.mask')
			.split(/^===$/gm)
			.map(function(test){
				
				return test
					.split(/^---$/gm)
					.map(function(x){
						return x.trim();
					});
			})
			.forEach(function(parts){
				
				eq(parts.length, 2, 'Test case expected. Source<->Result');
				
				var str = M.stringify(parts[0]);
				
				eq(str, parts[1]);
			});
	}
	
})